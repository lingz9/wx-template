/* pages/containerHy/containerHy.wxss */
.page{
  width: 100vw;
  min-height: 82vh;
  background: #ddd;
  position: relative;
  /* overflow: hidden; */
}
.cust_comp{
  position: absolute;
  top: 0;
  left: 100vw;
  width: 100vw;
  /* min-height: 82vh; */
  animation: pageInRight 600ms ease;
  animation-fill-mode: forwards;
}
/* .cust_comp.active{
  animation: pageInRight 600ms ease;
  animation-fill-mode: forwards;
} */
@keyframes pageInRight {
  from {
    left: 100vw;
  }
  to {
    left: 0;
  }
}
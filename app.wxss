/**app.wxss**/
/* 公共样式 */
view, navigator,scroll-view, block, button, form, input, checkbox, radio, label, image, text, textarea{
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  outline: none;
}
.pr{
  position: relative;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.clear{
  overflow: auto;
  _height: 1%;
}
page{
  width: 100%;
  padding-bottom: 100rpx;
}
.container {
  height: 100%;
}
.full{
  width: 100%;
  height: 100%;
}
.fw{
  width: 100%;
}